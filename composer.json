{
    "name": "altynbek07/qazaq-transliterator",
    "description": "Transliteration of the old Kazakh alphabet into a new one",
    "keywords": [
        "qazaq",
        "kazakh",
        "transliterator",
        "latin",
        "alphabet"
    ],
    "homepage": "https://github.com/altynbek07/qazaq-transliterator",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5|^9.0",
        "vimeo/psalm": "^3.11"
    },
    "authors": [
        {
            "name": "Altynbek",
            "email": "altynbek.kazezov.97@gmail.com",
            "homepage": "https://github.com/altynbek07",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Altynbek07\\QazaqTransliterator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Altynbek07\\QazaqTransliterator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
